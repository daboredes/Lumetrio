var discos = [
    ['El impulso', 2007, 13],
    ['On the night', 1990, 10],
    ['Born to run', 1975, 8]
];
discos[1][1]=1993;
discos.push(['Twenty One Pilots', 2009, 14, 'Twenty one Pilots']); // Agrega un array.
discos[0][3]='La Vela Puerca'; // Agrega elementos a las arrayas
discos[1][3]='Dire Straits';
discos[2][3]='Bruce Springsteen'



console.log(discos);
